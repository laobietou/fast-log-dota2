{
	"name": "fast-log-dota2",
	"displayName": "fast-log-dota2",
	"description": "output print() in ts and $.MSG in tsx",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.82.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fast-log-dota2.generator-log",
				"title": "Generator Log"
			}
		],
		"keybindings": [
			{
				"command": "fast-log-dota2.generator-log",
				"key": "ctrl+shift+l",
				"mac": "cmd+shift+l",
				"when": "editorTextFocus",
				"group": "navigation"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.82.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"eslint": "^8.47.0",
		"glob": "^10.3.3",
		"mocha": "^10.2.0",
		"typescript": "^5.1.6",
		"@vscode/test-electron": "^2.3.4"
	}
}
