{
	"name": "fast-log-dota2",
	"displayName": "Fast Log Dota2",
	"description": "auto-generate print() in .ts and $.Msg() in .tsx",
	"version": "1.0.2",
	"publisher": "laobietou",
	"engines": {
		"vscode": "^1.82.0"
	},
	"icon": "images/icon.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:typescriptreact"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fast-log-dota2.generator-log",
				"title": "Generator Log"
			},
			{
				"command": "fast-log-dota2.generator-annotation",
				"title": "Annotation Logs"
			},
			{
				"command": "fast-log-dota2.generator-unannotation",
				"title": "Unannotation Logs"
			}
		],
		"keybindings": [
			{
				"command": "fast-log-dota2.generator-log",
				"key": "ctrl+shift+l",
				"mac": "cmd+shift+l",
				"when": "editorTextFocus && resourceLangId == typescript || resourceLangId == typescriptreact"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "fast-log-dota2.generator-log",
					"when": "editorTextFocus && resourceLangId == typescript || resourceLangId == typescriptreact",
					"group": "1_modification"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.82.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"eslint": "^8.47.0",
		"glob": "^10.3.3",
		"mocha": "^10.2.0",
		"typescript": "^5.1.6",
		"@vscode/test-electron": "^2.3.4"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/laobietou/fast-log-dota2"
	}
}
